@model Impact.Website.Models.DemoOvertimeViewModel
@{
    ViewBag.Title = "Demo Overarbejde";
}
<div class="container">
    <h2>Demo kvartal</h2>
    <div class="row">
        <div class="col-md-4">
            @for (int i = 0; i < 5; i++)
            {
            <p>
                @Model.Weeks[i].GetDisplayNumber()
                @Html.TextBoxFor(m => m.Weeks[i].TotalHours)
            </p>
            }
        </div>
        <div class="col-md-4">
            @for (int i = 5; i < 10; i++)
            {
            <p>
                @Model.Weeks[i].GetDisplayNumber()
                @Html.TextBoxFor(m => m.Weeks[i].TotalHours)
            </p>
            }
        </div>
        <div class="col-md-4">
            @for (int i = 10; i < Model.Weeks.Count; i++)
            {
            <p>
                @Model.Weeks[i].GetDisplayNumber()
                @Html.TextBoxFor(m => m.Weeks[i].TotalHours)
            </p>
            }
        </div>
    </div>
</div>